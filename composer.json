{
  "name": "kasperhartwich/quickdns",
  "description": "QuickDNS library",
  "keywords": ["quickdns", "dns"],
  "license": "MIT",
  "authors": [
    {
      "name": "Kasper Hartwich",
      "email": "kasperhartwich@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/dom-crawler": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "QuickDns\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QuickDns\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "format": "vendor/bin/pint"
  }
}
